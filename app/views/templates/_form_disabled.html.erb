<%= form_for(@template, html: {class: "form-horizontal", role: "form"}) do |f| %>
	<div class="form-group">
		<%= f.label :name, 'name', :class => "col-lg-3 control-label" %>
		<div class="col-lg-5">
			<%= f.text_field :name, :class => "form-control input-sm", :disabled => true %>
		</div>
	</div>
	<div class="form-group">
		<%= f.label :image_location, 'image location', :class => "col-lg-3 control-label" %>
		<div class="col-lg-5">
			<%= f.text_field :image_location, :class => "form-control input-sm", :disabled => true %>
		</div>
	</div>
	<div class="form-group">
		<%= f.label :provider, 'provider', :class => "col-lg-3 control-label" %>
		<div class="col-lg-5">
			<%= f.text_field :provider, :class => "form-control input-sm", :disabled => true %>
		</div>
	</div>
	<div class="form-group">
		<%= f.label :os_name, 'os', :class => "col-lg-3 control-label" %>
		<div class="col-lg-5">
			<%= f.text_field :os_name, :class => "form-control input-sm", :disabled => true %>
		</div>
	</div>
	<div class="form-group">
		<%= f.label :os_version, 'os version', :class => "col-lg-3 control-label" %>
		<div class="col-lg-5">
			<%= f.text_field :os_version, :class => "form-control input-sm", :disabled => true %>
		</div>
	</div>
	<div class="form-group">
		<%= f.label :container_support, 'container support', :class => "col-lg-3 control-label" %>
		<div class="col-lg-5">
			<%= f.text_field :container_support, :class => "form-control input-sm", :disabled => true %>
		</div>
	</div>
	<div class="form-group">
		<%= f.label :version, 'version', :class => "col-lg-3 control-label" %>
		<div class="col-lg-5">
			<%= f.text_field :version, :class => "form-control input-sm", :disabled => true %>
		</div>
	</div>
<% end %>

